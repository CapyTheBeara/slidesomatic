{{#if youtube}}
    {{popcorn-video id  = 'media-view'
                    media = content}}
{{/if}}

{{#if soundcloud}}
    {{popcorn-media id = 'audio-view'
                    media = content}}
{{/if}}

{{#if editMode}}
    {{input-slider value  = scrubHighValue
                   action ='scrubHigh'}}
    {{input-slider value  = scrubMediumValue
                   action ='scrubMedium'}}
    {{input-slider value  = scrubLowValue
                   action ='scrubLow'}}
{{/if}}
